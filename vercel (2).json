{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "WEBHOOK_URL": "@webhook-url",
    "API_KEY_AI": "@api-key-ai",
    "PORT": "3000",
    "NODE_ENV": "production"
  }
}
